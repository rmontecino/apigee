<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-Set-Credentials">
    <DisplayName>AM-Set-Credentials</DisplayName>
    <Properties/>
    <!--<Set>
        <Headers>
            <Header name="clientId">paste-your-client-id-here</Header>
        </Headers>
    </Set>-->
    <AssignVariable>
        <Name>username</Name>
        <Value>paste-your-client-id-here</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>password</Name>
        <Value>paste-your-client-secret-here</Value>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>